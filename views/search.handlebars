<!DOCTYPE html>
<html>
    <head>
      <meta charset="utf-8">
      <title></title>
      <link rel="stylesheet" href="/style.css">
    </head>
    <body>
      <form class="" action="/search" method="get">
        <input type="text" name="current snippets">
        <button type="button" name="button">Search</button>

        <div class="wrapper" id="snippets">
         {{#snippets}}
           <a href="/snippets/add?id={{ _id }}">
             <p class="title">{{title}}</p>
           </a>
         {{/snippets}}
         <form class="add" action="/snippets/add" method="get">
           <button class='addButton' name="add"> add </button>
        </form>


    </body>
  </html>
